  {
    "table": "user_message",
    "comment": "用户聊天消息表",
    "key": "LOCAL_MESSAGE_ID",
    "fts": "FTS4",
    "cols": [
      {
        "name": "LOCAL_MESSAGE_ID",
        "type": "char(40)"
      },
      {
        "name": "FROM_ID",
        "type": "char(200)"
      },
      {
        "name": "LISTENED",
        "type": "TEXT"
      },
      {
        "name": "TO_ID",
        "type": "char(200)"
      },
      {
        "name": "CHATTER",
        "type": "char(200)"
      },
      {
        "name": "BODY",
        "type": "TEXT"
      },
      {
        "name": "TIME_MILLIS",
        "type": "INTEGER"
      },
      {
        "name": "SERVER_MESSAGE_ID",
        "type": "char(40)"
      },
      {
        "name": "FROM_NICK_NAME",
        "type": "TEXT"
      },
      {
        "name": "TO_NICK_NAME",
        "type": "TEXT"
      },
      {
        "name": "isread",//是否打开过对话
        "type": "INTEGER"
        //0为未打开，1为已打开，此字段为标志这条消息所属会话有没有打开，用于计算红点 2018-04-18；
        //如果被打开了，就会被置为1，用在消息被撤回，如果未读，要把红点去掉；
      },
      {
        "name": "unread_count",
        //对应会话中的未读数【单人：已读未读；多人：*人未读，全部已读】；
        "type": "TEXT"
      },
      {
        "name": "read_status",//
        //登陆人相对于此条消息是否已读,避免多次告诉消息服务器我读了这条消息，让服务器告诉对方我读了，此字段逻辑主要是为了减轻服务端并发压力；
        "type": "TEXT"
      },
      {
        "name": "read_switch",
        //是否已读的开关；
        "type": "TEXT"
      },
      {
        "name": "send_status",
        "type": "TEXT"
      }
    ]
  },
